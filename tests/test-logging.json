{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "[%(asctime)s] [%(levelname)-5s] [%(pathname)s]: %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stderr"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console"
   ]
  }
}
